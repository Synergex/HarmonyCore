{xfMethod(interface="TestDate",name="GetDate8",elb="TEST:testelb",id="GetDate8")}
subroutine GetDate8
{xfParameter(name="idx")}
req in      idx         ,d2                     ;;
{xfParameter(name="dstruct",structure="strDatetimestr")}
req out     a_date     ,strDatetimestr            ;;
endparams
;.subroutine getDate8
;	idx, n
;	a_date, a

record DateTimeStr                                          
  DTS_ALPHA ,A10  ; (1,10)                                                      
  DTS_DATE8 ,A8   ; (11,18)                                                     
  DTS_DATE6 ,D6   ; (19,24)                                                     
  DTS_TIME6 ,D6   ; (25,30)                                                     
  DTS_TIME4 ,D4   ; (31,34)                                                     
.proc
	clear DateTimeStr
	dts_alpha = "hey"
	dts_date6 = 0
	dts_time6 = 0
	dts_time4 = 0

	case idx of
	begincase
	0:	dts_date8 = "20050520"
	1: 	dts_date8 = "00      "
	2:      dts_date8 = "      00"
	3: 	dts_date8 = "0 0 0 0 "
	4: 	dts_date8 = "  000   "
	endcase
	a_date = DateTimeStr
	xreturn
.end

